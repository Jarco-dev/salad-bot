{
    "extends": "./node_modules/gts/",
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "no-trailing-spaces": [
            "error",
            {
                "skipBlankLines": true
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
