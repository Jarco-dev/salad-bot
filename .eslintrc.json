{
    "extends": "./node_modules/gts/",
    "rules": {
        "node/no-unpublished-import": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "quotes": [
            "error",
            "double"
        ],
        "no-trailing-spaces": [
            "error",
            {
                "skipBlankLines": true
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "lf"
            }
        ]
    }
}
