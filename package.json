{
    "name": "salad-bot",
    "version": "1.2.1",
    "description": "The discord bot for the salad server",
    "main": "dist/index.js",
    "engines": {
        "node": ">=16.9.0"
    },
    "scripts": {
        "lint": "gts lint",
        "fix": "gts fix",
        "build": "gts clean && tsc",
        "dev": "nodemon --watch \"src/**\" --ext \"ts\" --exec \"ts-node src/index.ts\"",
        "dev:proxy": "nodemon ---watch \"proxyServer/**\" --ext \"ts\" --exec \"node proxyServer/index.js\"",
        "start": "node dist/index.js",
        "start:proxy": "node proxyServer/index.js"
    },
    "author": "Jarco-dev",
    "dependencies": {
        "@prisma/client": "^4.1.1",
        "@sansamour/node-socks": "^1.0.0",
        "discord.js": "^14.1.2",
        "dotenv": "^16.0.3",
        "mineflayer": "^4.5.1",
        "moment": "^2.29.4",
        "socks": "^2.7.1"
    },
    "devDependencies": {
        "@types/node": "^16.11.47",
        "copyfiles": "^2.4.1",
        "gts": "^3.1.1",
        "nodemon": "^2.0.19",
        "prisma": "^4.1.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}
